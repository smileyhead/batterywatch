#: ../metadata.desktop
msgid "BatteryWatch"
msgstr "BatteryWatch"

# %1 is the version number.
#: ../contents/ui/main.qml
msgid "BatteryWatch v%1"
msgstr "BatteryWatch v%1"

#: ../contents/ui/main.qml
msgid "No connected devices"
msgstr "Nincsenek csatlakoztatott eszközök"

# %1 can be the device name or, in the case of multiple batteries, the battery label, or simply the word, ‘Battery’. %2 is the charge percentage value.
#: ../contents/ui/main.qml
msgid "%1: %2%"
msgstr "%1: %2%"

# The delimiter when listing multiple batteries in the same device.
#: ../contents/ui/main.qml
msgid ", "
msgstr ", "

# Used when there are multiple batteries in a device. %1 is the device name. %2 is the delimited list of the device's batteries and their charge percentages.
#: ../contents/ui/main.qml
msgid "%1 - %2"
msgstr "%1 - %2"

#: ../contents/ui/main.qml
msgid "All devices hidden"
msgstr "Minden eszköz rejtett"

# %1 is the charge percentage value.
#: ../contents/ui/main.qml
msgid "%1%"
msgstr "%1%"

#: ../contents/ui/main.qml
msgid "Device Battery Levels"
msgstr "Eszközök töltöttsége"

#: ../contents/ui/main.qml
msgid "Refresh"
msgstr "Frissítés"

#: ../contents/ui/main.qml
msgid "Refresh devices"
msgstr "Eszközlista frissítése"

#: ../contents/ui/main.qml ../contents/ui/providers/CompanionProvider.qml
msgid "Unknown Device"
msgstr "Ismeretlen eszköz"

#: ../contents/ui/main.qml
msgid "Battery"
msgstr "Akkumulátor"

# %1 is the battery label or simply the word, ‘Battery’. %2 is the charge percentage value. %3 is a Unicode lightning symbol displayed when the device is charging.
#: ../contents/ui/main.qml
msgid "%1: %2%%3"
msgstr "%1: %2%%3"

#: ../contents/ui/main.qml
msgid "Disconnect"
msgstr "Leválasztás"

#: ../contents/ui/main.qml
msgid "Disconnect device"
msgstr "Eszköz leválasztása"

#: ../contents/ui/main.qml
msgid "Hide"
msgstr "Elrejtés"

#: ../contents/ui/main.qml
msgid "Show"
msgstr "Megjelenítés"

#: ../contents/ui/main.qml
msgid "Hide from tray"
msgstr "Elrejtés a paneltálcáról"

#: ../contents/ui/main.qml
msgid "Show in tray"
msgstr "Megjelenítés a paneltálcán"

#: ../contents/ui/main.qml
msgid "No connected devices with battery info found"
msgstr ""
"Jelenleg egy csatlakoztatott eszköz sem közöl "
"akkumulátor-töltöttségi adatokat"

#: ../contents/ui/providers/CompanionProvider.qml
msgid "BatteryWatch: Companion service disconnected"
msgstr "BatteryWatch: Megszakadt a kapcsolat a társszolgáltatással"

#: ../contents/ui/providers/CompanionProvider.qml
msgid "BatteryWatch: Failed to parse companion data:"
msgstr "A társadatok feldolgozása meghiúsult:"

# Used when a device is known by BatteryWatch to be misreported by UPower. %1 is the device's model name. %2 is the erroneously reported device type (e.g.: ‘mouse’). %3 is the correct device type.
#: ../contents/ui/providers/UPowerProvider.qml
msgid "BatteryWatch: Applying UPower device override for '%1': %2 -> %3"
msgstr "BatteryWatch: UPower-eszköz felülírása. „%1”: %2 -> %3"
